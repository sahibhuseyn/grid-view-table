{
  "name": "assurrussa/grid-view-table",
  "description": "Mini grid view table for laravel.",
  "keywords": [
    "laravel",
    "gridView",
    "Vue",
    "Table"
  ],
  "type": "package",
  "homepage": "https://github.com/sahibhuseyn/grid-view-table",
  "license": "MIT",
  "authors": [
    {
      "name": "AssurrussA",
      "email": "assurrussa@yandex.ru"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "nunomaduro/collision": "~1.1",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^10.1",
    "symfony/thanks": "^1.0",
    "barryvdh/laravel-debugbar": "^3.0",
    "barryvdh/laravel-ide-helper": "^2.2",
    "squizlabs/php_codesniffer": "2.*"
  },
  "support": {
    "source": "https://github.com/assurrussa/grid-view-table",
    "issues": "https://github.com/assurrussa/grid-view-table/issues"
  },
  "autoload": {
    "psr-4": {
      "Assurrussa\\GridView\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Assurrussa\\GridView\\GridViewServiceProvider"
      ],
      "aliases": {
        "AmiGrid": "Assurrussa\\GridView\\Facades\\GridViewFacade"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "symfony/thanks": true
    }
  }
}
